<tr id="playlist{{@index}}">
  <td>
    {{uppercase title}}
  </td>
  <td class="center aligned">
    <button class="ui icon button" title="Edit playlist" id="showEdit{{@index}}">
      <i class="ui pencil icon"></i>
    </button>
  </td>
  <td class="center aligned">
    <a href="/playlist/{{playlistId}}/deletesong/{{id}}" class="ui icon button"  title="Delete Playlist">
      <i class="ui trash icon"></i>
    </a>
  </td>
</tr>